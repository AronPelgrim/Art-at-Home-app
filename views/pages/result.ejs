<!DOCTYPE html>
<html>
    <%- include('../partials/head') %>
    <body>
        <%- include('../partials/header') %>
        <main>
            <%- include('../partials/form') %>
            <h1 class="visible"><%- data[1].principalOrFirstMaker %></h1>
            <ul class="">
                <% data.forEach(function(paintings) { %>
                    <li>
                        <img src=<%- paintings.webImage.url.slice(0, -3)+"=s1000" %>>
                        <p><%- paintings.longTitle %></p>
                    </li>
                <% }) %>  
            </ul>
        </main>
    </body>
</html>